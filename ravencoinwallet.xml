<?xml version="1.0"?>
<Container version="2">
  <Name>RavencoinWalletGUI</Name>
  <Repository>angelics/unraid-docker-ravencoin-wallet</Repository>
  <Registry>https://hub.docker.com/r/angelics/unraid-docker-ravencoin-wallet</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>RavenCoin wallet with GUI</Overview>
  <Category>Other: Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:5800]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/angelics/unraid-docker-ravencoin-wallet/raw/master/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <Description>RavenCoin wallet with GUI</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5900</HostPort>
        <ContainerPort>5900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5800</HostPort>
        <ContainerPort>5800</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8767</HostPort>
        <ContainerPort>8767</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/RavenCoinWallet/</HostDir>
      <ContainerDir>/storage/.raven/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Labels/>
  <Config Name="port" Target="5900" Default="" Mode="tcp" Description="Container Port: 5900" Type="Port" Display="always" Required="false" Mask="false">5900</Config>
  <Config Name="port" Target="5800" Default="" Mode="tcp" Description="Container Port: 5800" Type="Port" Display="always" Required="false" Mask="false">5800</Config>
  <Config Name="port" Target="8767" Default="" Mode="8767" Description="Container Port: 8767" Type="Port" Display="always" Required="false" Mask="false">8767</Config>
  <Config Name="RavenCoin Blockchain Path" Target="/storage/.raven/" Default="" Mode="rw" Description="Container Path: /storage/.raven/" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/RavenCoinWallet/</Config>
</Container>
